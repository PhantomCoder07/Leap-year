#include <stdio.h>
int leap (int y)
{
    if (y%400==0 || (y%4==0 && y%100!=0))
        return 1;
    else
        return 0;
}
int main()
{
    int year;
    printf ("Enter the year: ");
    scanf ("%d",&year);
    if (leap(year))
        printf ("%d is Leap Year.\n",year);
    else
        printf ("%d is not Leap Year.\n",year);
    return 0;
}
