#include <stdio.h>
int main()
{
    int year;
    printf ("Enter the year: ");
    scanf ("%d",&year);
    switch (year%100)
    {
    case 0:
        switch (year%400)
        {
        case 0:
            printf ("%d is Leap Year.",year);
            break;
        default:
            printf ("%d is not Leap Year.",year);
        }
        break;
    default:
        switch (year%4)
        {
        case 0:
            printf ("%d is Leap Year.",year);
            break;
        default:
            printf ("%d is not Leap Year.",year);
        }
    }
    return 0;
}
